msgid "Architecture"
msgstr "Arquitectura"

#: data/templates/cloud-tab.html:15
msgid "Root store"
msgstr "Depósito raíz"

#: data/templates/cloud-tab.html:16
msgid "AMI ID"
msgstr "ID AMI"

