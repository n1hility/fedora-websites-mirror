#: data/content/verify.html:59
msgid "Fedora 23"
msgstr "Fedora 23"

#: data/content/verify.html:60
msgid "Fedora 23 secondary arches (AArch64, PPC64, PPC64le, s390 and s390x)"
msgstr ""

#: data/content/verify.html:61
msgid "Fedora 22"
